<h2>Users</h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Signup Date</th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
  <tr>
    <td><%= link_to user.full_name, admin_user_path(user) %></td>
    <td><%=user.email%></td>
    <td><%=user.created_at%></td>

  </tr>
  <% end %>    
  </tbody>
</table>

